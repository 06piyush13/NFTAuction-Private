{
  "compilerOptions": {
    "composite": true,
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "vite.config.ts"
  ]
}