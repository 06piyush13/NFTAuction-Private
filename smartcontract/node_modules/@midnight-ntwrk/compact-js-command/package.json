  "sideEffects": [],
  "author": "IOHK",
  "dependencies": {
    "@effect/cli": "^0.73.1",
    "@effect/cluster": "^0.56.1",
    "@effect/experimental": "^0.56.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/platform-node-shared": "^0.57.1",
    "@effect/printer": "^0.47.0",
    "@effect/printer-ansi": "^0.47.0",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "@effect/typeclass": "^0.38.0",
    "@effect/workflow": "^0.11.3",
    "@midnight-ntwrk/compact-js": "^2.4.0",
    "@midnight-ntwrk/compact-js-node": "^2.4.0",
    "@midnight-ntwrk/compact-runtime": "0.14.0",
    "@midnight-ntwrk/ledger-v7": "7.0.0",
    "@midnight-ntwrk/platform-js": "^2.2.0",
    "@midnight-ntwrk/wallet-sdk-address-format": "3.0.0",
    "effect": "^3.19.15",
    "json5": "^2.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1"
  },
  "main": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./effect": {
      "types": "./dist/dts/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "default": "./dist/cjs/effect/index.js"
    },
    "./effect/CommandConfigProvider": {
      "types": "./dist/dts/effect/CommandConfigProvider.d.ts",
      "import": "./dist/esm/effect/CommandConfigProvider.js",
      "default": "./dist/cjs/effect/CommandConfigProvider.js"
    },
    "./effect/CompiledContractReflection": {
      "types": "./dist/dts/effect/CompiledContractReflection.d.ts",
      "import": "./dist/esm/effect/CompiledContractReflection.js",
      "default": "./dist/cjs/effect/CompiledContractReflection.js"
    },
    "./effect/ConfigCompilationError": {
      "types": "./dist/dts/effect/ConfigCompilationError.d.ts",
      "import": "./dist/esm/effect/ConfigCompilationError.js",
      "default": "./dist/cjs/effect/ConfigCompilationError.js"
    },
    "./effect/ConfigCompiler": {
      "types": "./dist/dts/effect/ConfigCompiler.d.ts",
      "import": "./dist/esm/effect/ConfigCompiler.js",
      "default": "./dist/cjs/effect/ConfigCompiler.js"
    },
    "./effect/ConfigError": {
      "types": "./dist/dts/effect/ConfigError.d.ts",
      "import": "./dist/esm/effect/ConfigError.js",
      "default": "./dist/cjs/effect/ConfigError.js"
    },
    "./effect/index": {
      "types": "./dist/dts/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "default": "./dist/cjs/effect/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "effect": [
        "./dist/dts/effect/index.d.ts"
      ],
      "effect/CommandConfigProvider": [
        "./dist/dts/effect/CommandConfigProvider.d.ts"
      ],
      "effect/CompiledContractReflection": [
        "./dist/dts/effect/CompiledContractReflection.d.ts"
      ],
      "effect/ConfigCompilationError": [
        "./dist/dts/effect/ConfigCompilationError.d.ts"
      ],
      "effect/ConfigCompiler": [
        "./dist/dts/effect/ConfigCompiler.d.ts"
      ],
      "effect/ConfigError": [
        "./dist/dts/effect/ConfigError.d.ts"
      ],
      "effect/index": [
        "./dist/dts/effect/index.d.ts"
      ]
    }
  }
}