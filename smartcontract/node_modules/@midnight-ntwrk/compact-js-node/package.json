{
  "name": "@midnight-ntwrk/compact-js-node",
  "version": "2.4.0",
  "description": "Platform specific types and utilities for working with the Compact language on a Node.js runtime",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/midnight-ntwrk/artifacts.git"
  },
  "sideEffects": [],
  "author": "IOHK",
  "dependencies": {
    "@effect/cluster": "^0.56.1",
    "@effect/experimental": "^0.56.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "@effect/workflow": "^0.11.3",
    "effect": "^3.19.15",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@midnight-ntwrk/compact-js": ">=2.4.0"
  },
  "main": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./effect": {
      "types": "./dist/dts/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "default": "./dist/cjs/effect/index.js"
    },
    "./effect/ZKFileConfiguration": {
      "types": "./dist/dts/effect/ZKFileConfiguration.d.ts",
      "import": "./dist/esm/effect/ZKFileConfiguration.js",
      "default": "./dist/cjs/effect/ZKFileConfiguration.js"
    },
    "./effect/index": {
      "types": "./dist/dts/effect/index.d.ts",
      "import": "./dist/esm/effect/index.js",
      "default": "./dist/cjs/effect/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "effect": [
        "./dist/dts/effect/index.d.ts"
      ],
      "effect/ZKFileConfiguration": [
        "./dist/dts/effect/ZKFileConfiguration.d.ts"
      ],
      "effect/index": [
        "./dist/dts/effect/index.d.ts"
      ]
    }
  }
}